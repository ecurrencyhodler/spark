const { SparkWallet, BareSparkSigner } = require("@buildonspark/bare", { with: { imports: "./imports.json" } });

function log(prm) { prm.then((result) => { console.log("Result:", result); }).catch((error) => { console.error("Error:", error); }); }
function assignToGlobal(prm, name) { prm.then((result) => { global[name] = result; }).catch((error) => { console.error("Error assigning result to global:", error); }); }

assignToGlobal(SparkWallet.initialize({ signer: new BareSparkSigner(), mnemonicOrSeed: undefined }), "w1r");
